"code 3 default null"
import 'dart:io';

String say(String from, String message, {String to, String appName}){
  return from + " say " + massage + ((to != null) ? " to " + to : "") + ((appName != null) ?
  " via " +appName : "");
  }
  
  String say2(String from, String message, [String to, String appName]){
  return from + " say " + massage + ((to != null) ? " to " + to : "") + ((appName != null) ?
  " via " +appName : "");
  }
  
 main(List<String> arguments) {
   print(say(Elsa", "Hai", to: "Doloris", appName: "whatsapp"));
   print2(say(Elsa", "Hai","Doloris"));
   
  }
  
  
  output
  Elsa say Hai to Doloris via whatsapp
  Elsa say Hello to Doloris
 
 
 




 
 
 
