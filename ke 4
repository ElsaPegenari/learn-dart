 16.0
 
 
 
"code fungsi 1 tanpa pengembalian"
import 'dart:io';

double luas segiempat(double panjang, double lebar) {
  double hasil;
  hasil = panjang * lebar;
  return hasil;
  
 }
 
void sapa_penonton()
print ("Halo penonton semua!");
}
