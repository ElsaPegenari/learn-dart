"code fungsi 1"

import 'dart:io';

double luas segiempat(double panjang, double lebar) {
  double hasil;
  hasil = panjang * lebar;
  return hasil;
  
 }
main(List<String> arguments) {
 double p, l, luas;
 
 p = double.tryPerse(stdin.realLineSync());
 l = double.tryPerse(stdin.realLineSync());
 
 luas = luas_segiempat(p, l);
 
 print(luas);
 }
 
 output
 4
